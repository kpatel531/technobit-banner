<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Technobit_Banner::banner" title="Technobit Banner" sortOrder="100">
                    <resource id="Technobit_Banner::manage" title="Manage" sortOrder="10">
                        <resource id="Technobit_Banner::save" title="Save" sortOrder="10" />
                        <resource id="Technobit_Banner::delete" title="Delete" sortOrder="20" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Technobit_Banner::config" title="Banner config" translate="title" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
